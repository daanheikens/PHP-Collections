{
  "name": "heike/php-fi",
  "description": "Functional Interface for PHP",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.3.8"
    }
  },
  "require": {
    "ext-xsl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.0.*",
    "edgedesign/phpqa": "1.23.*",
    "phpstan/phpstan": "0.12.*",
    "jakub-onderka/php-parallel-lint": "1.0.*"
  },
  "autoload": {
    "psr-4": {
      "Fi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
