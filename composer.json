{
  "name": "heike/php-fi",
  "description": "PHP collections with functional interface for PHP",
  "keywords": ["data-structures", "functional-programming", "php-ds", "collection"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Daan Heikens",
      "email": "heikens1408@outlook.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.4.3"
    }
  },
  "require": {
    "thecodingmachine/safe": "^1.0",
    "php-ds/php-ds": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "8.0.*",
    "squizlabs/php_codesniffer": "^3.5",
    "thecodingmachine/phpstan-safe-rule": "^1.0",
    "thecodingmachine/phpstan-strict-rules": "^0.12.0",
    "jakub-onderka/php-console-highlighter": "^0.4.0",
    "edgedesign/phpqa": "^1.23",
    "phpstan/phpstan": "^0.12.18",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "phpmd/phpmd": "@stable",
    "nette/neon": "^3.1",
    "sensiolabs/security-checker": "^6.0"
  },
  "suggest": {
    "ext-ds": "More efficient data structures than php-ds/php-ds"
  },
  "scripts": {
    "lint": "parallel-lint --exclude vendor src/ tests/",
    "csfix": "phpcbf src/",
    "cscheck": "phpcs src/",
    "phpstan": "phpstan analyse src/ -c phpstan.neon --level 7 --no-progress --memory-limit 2048M",
    "phpqa": "phpqa"
  },
  "autoload": {
    "psr-4": {
      "Fi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FI\\": "tests/"
    }
  }
}
