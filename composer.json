{
  "name": "heike/php-fi",
  "description": "Functional Interface for PHP",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.4.3"
    }
  },
  "require": {
    "ext-ds": "*",
    "thecodingmachine/safe": "^1.0",
    "php-ds/php-ds": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "8.0.*",
    "phpstan/phpstan": "dev-master",
    "php-parallel-lint/php-parallel-lint": "^1.1.0",
    "squizlabs/php_codesniffer": "^3.5",
    "thecodingmachine/phpstan-safe-rule": "^1.0",
    "thecodingmachine/phpstan-strict-rules": "^0.12.0",
    "jakub-onderka/php-console-highlighter": "^0.4.0"
  }, 
  "scripts": {
    "lint": "parallel-lint --exclude vendor src/ tests/",
    "csfix": "phpcbf src/",
    "cscheck": "phpcs src/",
    "phpstan": "phpstan analyse src/ -c phpstan.neon --level 7 --no-progress -vvv --memory-limit 2048M"
  },
  "autoload": {
    "psr-4": {
      "Fi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FI\\Tests\\": "tests/"    }
  }
}
